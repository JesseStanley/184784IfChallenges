<Window x:Class="_184784IfChallenges.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_184784IfChallenges"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <Label x:Name="lblNumber" Content="Choose A Number" />
        <TextBox x:Name="txtNumber"></TextBox>
        <TextBox x:Name="txtNumber2"></TextBox>
        <Button x:Name="btnNumber" Click="btnNumber_Click" Content="Max Number" />
    </StackPanel>
</Window>
